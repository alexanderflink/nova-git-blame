{
  "identifier": "alexanderflink.gitblame",
  "name": "Git blame",
  "organization": "Alexander Flink",
  "description": "Git blame command for Nova.",
  "version": "1.0",
  "categories": ["commands"],
  "main": "index.js",
  "bugs": "https://github.com/alexanderflink/nova-git-blame/issues",
  "repository": "https://github.com/alexanderflink/nova-git-blame.git",
  "activationEvents": ["onCommand:nova-git-blame.blame"],
  "entitlements": {
    "process": true
  },
  "commands": {
    "editor": [
      {
        "title": "Git blame",
        "command": "nova-git-blame.blame",
        "shortcut": "cmd-shift-b",
        "when": "editorHasFocus"
      }
    ]
  }
}
